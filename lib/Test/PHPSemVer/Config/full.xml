<?xml version="1.0" ?>
<phpsemver
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/sourcerer-mike/phpsemver/3.2.0/etc/phpsemver.xsd"
        title="My own versioning">
    <RuleSet name="first">
        <Trigger>
            <Classes>
                <IsRemoved/>
            </Classes>
            <Functions>
                <IsRemoved/>
            </Functions>
        </Trigger>
    </RuleSet>
    <RuleSet name="second">
        <Trigger>
            <Classes>
                <IsAdded/>
            </Classes>
            <Functions>
                <IsAdded/>
            </Functions>
        </Trigger>
    </RuleSet>
    <RuleSet name="last">
    </RuleSet>
    <Filter>
        <Blacklist>
            <Pattern>^pattern-test/blacklist/.*</Pattern>
        </Blacklist>
        <Whitelist>
            <Pattern>^pattern-test/whitelist/.*</Pattern>
        </Whitelist>
    </Filter>
</phpsemver>